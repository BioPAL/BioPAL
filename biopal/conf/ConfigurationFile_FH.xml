<ConfigurationL2FH version="1.0">
	<EstimateFH>
		<EstimationValidValuesLimits unit='m'>
			<min>5</min>
			<max>100</max>
		</EstimationValidValuesLimits>
		<SpectralShiftFiltering>False</SpectralShiftFiltering>
		<ProductResolution unit="m">200</ProductResolution>
		<KZ_thresholds unit="1/m">
			<min>0.01</min>
			<max>0.19</max>
		</KZ_thresholds>
		<ModelParameters>
			<MaximumHeight unit="m">61</MaximumHeight>
			<NumberOfExtinctionValue>51</NumberOfExtinctionValue>
			<NumberOfGroundVolumeRatioValue>100</NumberOfGroundVolumeRatioValue>
			<NumberOfTemporalDecorrelationValue>21</NumberOfTemporalDecorrelationValue>
		</ModelParameters>
		<MedianFactor>5</MedianFactor>
	</EstimateFH>
	<EnableResampling>False</EnableResampling>
	<ComputeGeometry>False</ComputeGeometry>
	<ApplyCalibrationScreen>False</ApplyCalibrationScreen>
	<DEMflattening>False</DEMflattening>
	<MultilookHeadingCorrection>True</MultilookHeadingCorrection>
	<SaveBreakpoints>True</SaveBreakpoints>
	<DeleteTemporaryFiles>False</DeleteTemporaryFiles>
</ConfigurationL2FH>
